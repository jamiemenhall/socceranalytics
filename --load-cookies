<script type="text/javascript">
$('.am-form-login-switch').click(function() {
    $('.am-login-form-wrapper').toggle();
    $('.am-sendpass-form-wrapper').toggle();

    if ($('.am-login-form-wrapper').is(':visible'))
        $('.am-body-content h1:first').text('Sign In');
    else
        $('.am-body-content h1:first').text('Forgotten Password');
    
    $('.am-login-form-wrapper .errors, .am-sendpass-form-wrapper .errors').remove();
});

$('.visual-only-customer-type > input').change(function() {
    $('.am-login-form-wrapper, .am-new-form-wrapper').toggle();
    $('#returning-visual-only-customer-returning').prop("checked", true);
    $('#new-visual-only-customer-new').prop("checked", true);
});
</script></div>
